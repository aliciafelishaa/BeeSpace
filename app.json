{
  "expo": {
    "name": "BeeSpace",
    "slug": "BeeSpace",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo-beespace-white.png",
    "scheme": "beespace",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "bundleIdentifier": "com.beespace",
      "supportsTablet": true
    },
    "android": {
      "package": "com.beespace",
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/share.png",
        "backgroundImage": "./assets/images/share.png",
        "monochromeImage": "./assets/images/share.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "bundler": "metro",
      "favicon": "./assets/images/share.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "dabdda42-5987-4bf8-a2bc-c4c7859c0261"
      }
    }
  }
}
